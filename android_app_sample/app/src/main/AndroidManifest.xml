<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gaa.iaa.sample">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gaa_iaa_sdk">
        <activity android:name="com.gaa.iaa.sample.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- OneStore InApp Ads meta-data start -->
        <meta-data android:name="iaa:appKey" android:value="APP_KEY"/>
        <meta-data android:name="iaa:appHashKey" android:value="APP_HASH_KEY"/>
        <!-- OneStore InApp Ads meta-data end -->
    </application>

</manifest>